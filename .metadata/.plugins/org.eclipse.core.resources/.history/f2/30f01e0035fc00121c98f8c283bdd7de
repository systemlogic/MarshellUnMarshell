package com;

import org.springframework.context.ApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

public class Hello {
	public static void main(String...str){
		ApplicationContext acx = new ClassPathXmlApplicationContext(new String[] {"Hello.xml"});
		helloBean hb = (helloBean)acx.getBean("Hello" ,helloBean.class);;
	}
}
