package com.annotation.bean;

public class Address {
	String locality;
	String stete;
	public Address(){
		this.locality = "Pawani oakridge";
		this.stete = "Karanataka";
	}
	public Address(String loc,String state){
		this.locality = loc;
		this.stete = state;
	}
	public String getLocality() {
		return locality;
	}
	public void setLocality(String locality) {
		this.locality = locality;
	}
	public String getStete() {
		return stete;
	}
	public void setStete(String stete) {
		this.stete = stete;
	}
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return " living in " + this.getLocality() + " " + this.getStete() ;
	}
}
