<?xml version="1.0" encoding="UTF-8"?>
<sdk version="1.5.0" min-version="1.5.0"
     href="http://cloudbees-downloads.s3.amazonaws.com/sdk/cloudbees-sdk-1.5.0-bin.zip">
    <link rel="alternate" href="http://wiki.cloudbees.com/bin/view/RUN/BeesSDK" type="text/html"/>

    <application-templates>
        <application-template id="basic" href="http://cloudbees-downloads.s3.amazonaws.com/app-templates/basic-1.0.zip"/>
    </application-templates>

    <plugins>
        <plugin artifact="org.cloudbees.sdk.plugins:app-plugin:1.5.2" required="true" force-upgrade="true"/>
        <plugin artifact="org.cloudbees.sdk.plugins:db-plugin:1.3.2" required="true" force-upgrade="true"/>
        <plugin artifact="org.cloudbees.sdk.plugins:ant-plugin:1.3.0" required="true" force-upgrade="true"/>
        <plugin artifact="org.cloudbees.sdk.plugins:config-plugin:1.3.2" required="true" force-upgrade="true"/>
        <plugin artifact="com.cloudbees.sdk.plugins:service-plugin:1.2.2" required="true" force-upgrade="true"/>

        <plugin artifact="com.cloudbees.sdk.plugins:server-plugin:1.2.1" force-upgrade="true"/>
        <plugin artifact="com.cloudbees.sdk.plugins:newrelic-plugin:1.1.0" force-upgrade="true"/>
    </plugins>

</sdk>
